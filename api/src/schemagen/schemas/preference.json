          "courseId": {
            "type": "number"
          },
          "ability": {
            "type": "string",
            "enum": ["ABLE", "WITH_DIFFICULTY"]
          },
          "willingness": {
            "type": "string",
            "enum": ["WILLING", "UNWILLING", "VERY_WILLING"]
          }
        }
      }
    },
    "additionalDetailes": {},
    "availability": {},
    "load": {}
  },
  "$schema": "http://json-schema.org/schema#"
}
