{
  "required": ["code", "subject", "term"],
  "type": "object",
  "properties": {
    "subject": {
      "type": "string",
      "description": "Course subject, e.g. SENG, CSC"
    },
    "code": {
      "type": "string",
      "description": "Course code, e.g. 499,310"
    },
    "term": {
      "type": "string",
      "description": "Term course is offered in",
      "enum": ["FALL", "SPRING", "SUMMER"],
      "$schema": "http://json-schema.org/schema#"
    }
  },
  "$schema": "http://json-schema.org/schema#"
}
