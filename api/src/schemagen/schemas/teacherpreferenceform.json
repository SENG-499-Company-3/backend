{
  "type": "object",
  "required": ["_id","peng", "courses", "email", "start", "end"],
  "properties": {
    "_id": {
      "type": "string"
    },
    "peng": {
      "type": "string",
      "description": "Professional Engineer certified"
    },
    "courses": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "Course name",
        "example": "Introduction to Python"
      }
    },
    "email": {
      "type": "string",
      "format": "email",
      "example": "user@email.com"
    },
    "start": {
      "type": "string",
      "description": "Start date of the course"
    },
    "end": {
      "type": "string",
      "description": "End date of the course"
    }
  },
  "$schema": "http://json-schema.org/schema#"
}