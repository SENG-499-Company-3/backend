{
  "type": "object",
  "required": ["peng", "courses", "email", "start", "end"],
  "properties": {
    "peng": {
      "type": "boolean",
      "description": "Professional Engineer certified"
    },
    "courses": {
      "type": "object",
      "required": ["subject", "code", "term"],
      "properties": {
        "subject": {
          "type": "string",
          "description": "Course subject, e.g. SENG, CSC"
        },
        "code": {
          "type": "string",
          "description": "Course code, e.g. 499,310"
        },
        "term": {
          "type": "string",
          "enum": ["FALL", "SPRING", "SUMMER"],
          "description": "Term course is offered in"
        }
      }
    },
    "email": {
      "type": "string",
      "format": "email",
      "example": "user@email.com"
    },
    "start": {
      "type": "string",
      "format": "date-time",
      "description": "Start date of the course"
    },
    "end": {
      "type": "string",
      "format": "date-time",
      "description": "End date of the course"
    }
  },
  "$schema": "http://json-schema.org/schema#"
}
